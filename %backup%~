server {
  server_name new-supertouch.local;

  access_log /home/guto/public/new-supertouch.local/log/access.log;
  error_log /home/guto/public/new-supertouch.local/log/error.log;
  # error_log /home/guto/public/new-supertouch.local/log/error.log debug;

  root /home/guto/public/new-supertouch.local/public/;

  client_max_body_size 20M;


  location / {
    index index.html index.php;
  }

  location ~ \.php$ {
    include /etc/nginx/fastcgi_params;
    fastcgi_pass unix:/var/run/php-fastcgi/php-fastcgi.socket;
    fastcgi_index index.php;
    fastcgi_param SCRIPT_FILENAME /home/guto/public/new-supertouch.local/public/$fastcgi_script_name;
  }

   # if (!-e $request_filename) {
   #    rewrite "^/[0-9]{4}/[0-9]{2}/[0-9]{2}/(.+)$" /$1 permanent;
   #    rewrite ^(.+)$ /index.php?q=$1 last;
   # }

}
